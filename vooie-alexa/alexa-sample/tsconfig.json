{
    "compilerOptions": {
        "lib": [
            "es2022"
        ],
        "baseUrl": "src",
        "outDir": "lambda",
        "sourceMap": true,
        "experimentalDecorators": true,
        "module": "commonjs",
        "target": "es2022",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "resolveJsonModule": true
    }
}